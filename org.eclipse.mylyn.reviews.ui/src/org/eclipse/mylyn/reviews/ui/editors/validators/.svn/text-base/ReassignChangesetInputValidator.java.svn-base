package org.eclipse.mylyn.reviews.ui.editors.validators;

import org.eclipse.jface.dialogs.IInputValidator;

/**
 * Input validator for text fields that must not be empty.
 * 
 * @author Stefan Reiterer
 * 
 */
public class ReassignChangesetInputValidator implements IInputValidator {

	/**
	 * Check validity of input field.
	 */
	public String isValid(String name) {
		if (!name.equals(""))
			return null;
		else
			return "";
	}

}
